Cell[CellGroupData[{

Cell["Sphaleron in the Higgs Triplet Model", "Section",
 CellChangeTimes->{{3.8979033422625723`*^9, 
  3.8979033499507017`*^9}},ExpressionUUID->"c8e5ff6b-32cd-4cb3-83c9-\
9193f3152bd1"],

Cell[BoxData[
 RowBox[{"(*", " ", 
  RowBox[{
   RowBox[{
    RowBox[{
    "The", " ", "sphaleron", " ", "configuration", " ", "in", " ", "the", " ",
      "Higgs", " ", "Triplet", " ", "Model", " ", "depends", " ", "on", " ", 
     "five", " ", 
     RowBox[{"parameters", ":", " ", "\[Rho]1"}]}], "=", 
    RowBox[{"\[Lambda]", "/", 
     SuperscriptBox["g", "2"]}]}], ";", " ", 
   RowBox[{"\[Rho]2", "=", 
    RowBox[{"2", 
     RowBox[{
      SuperscriptBox["\[Lambda]\[CapitalDelta]", "2"], "/", 
      SuperscriptBox["g", "2"]}]}]}], ";", 
   RowBox[{"\[Rho]3", "=", 
    RowBox[{
     SuperscriptBox["v\[CapitalDelta]", "2"], "/", 
     SuperscriptBox["v\[Phi]", "2"]}]}], ";", " ", 
   RowBox[{"\[Rho]4", "=", 
    RowBox[{
     SuperscriptBox["\[Kappa]", "2"], "/", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["g", "2"], 
       SuperscriptBox["v\[Phi]", "2"]}], ")"}]}]}], ";", 
   RowBox[{"\[Rho]5", "=", 
    RowBox[{
     SuperscriptBox["M\[CapitalDelta]", "2"], "/", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["g", "2"], 
       SuperscriptBox["v\[Phi]", "2"]}], ")"}]}]}]}], " ", "*)"}]], "Input",
 CellChangeTimes->{{3.897992896494272*^9, 3.897993129406828*^9}, {
   3.8979931832372694`*^9, 3.8979931842226243`*^9}, 3.8979932973493376`*^9},
 CellLabel->"In[26]:=",ExpressionUUID->"32413cef-d6ff-4900-8017-11b60a0e8add"],

Cell[BoxData[
 RowBox[{"(*", " ", 
  RowBox[{
   RowBox[{
   "To", " ", "calculate", " ", "the", " ", "sphaleron", " ", "configuration",
     " ", "in", " ", "the", " ", "Higgs", " ", "Triplet", " ", "Model"}], ",", 
   RowBox[{
   "please", " ", "run", " ", "the", " ", "function", " ", "SphaleronHTM"}]}],
   " ", "*)"}]], "Input",
 CellChangeTimes->{{3.8979931336379*^9, 3.897993190836944*^9}},
 CellLabel->"In[27]:=",ExpressionUUID->"27f52907-14c5-4f6a-a2c8-47cab2190cba"],

Cell[CellGroupData[{

Cell["\<\
Sphaleron configuration calculated using  the spectral method\
\>", "Subsection",
 CellChangeTimes->{{3.897903361977828*^9, 
  3.897903373727624*^9}},ExpressionUUID->"17e78920-2eb0-4b5d-809a-\
7ab428003993"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SphaleronHTM", "[", 
    RowBox[{
    "\[Rho]1_", ",", "\[Rho]2_", ",", "\[Rho]3_", ",", "\[Rho]4_", ",", 
     "\[Rho]5_"}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"energyHTM", "=", "10"}], ";", 
        RowBox[{"count", "=", "0"}]}], ",", 
       RowBox[{
        RowBox[{"energyHTM", ">", "3"}], "&&", 
        RowBox[{"count", "<", "10"}]}], ",", 
       RowBox[{"count", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\[Beta]", "=", 
         RowBox[{"1", "+", 
          RowBox[{"2", "\[Rho]3"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"n", "=", "60"}], ";", 
        RowBox[{"(*", 
         RowBox[{"number", " ", "of", " ", "grid", " ", "points"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"a", "=", "30"}], ";", 
        RowBox[{"(*", 
         RowBox[{"cut", "-", 
          RowBox[{
          "off", " ", "of", " ", "the", " ", "sphaleron", " ", "distance", 
           " ", "parameter", " ", "\[Xi]"}]}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"n", " ", "and", " ", "a", " ", "can", " ", "be", " ", 
           RowBox[{"adjusted", ".", " ", "Basically"}]}], ",", " ", 
          RowBox[{
          "to", " ", "evaluate", " ", "the", " ", "sphaleron", " ", 
           "configuration", " ", "with", " ", "large", " ", "\[Rho]1", " ", 
           "more", " ", "accurately"}], ",", " ", 
          RowBox[{
          "one", " ", "should", " ", "increase", " ", "n", " ", "and", " ", 
           "decrease", " ", "a"}]}], "*)"}], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Chebyshev", " ", "grid", " ", "points"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"x", "[", "j", "]"}], "=", 
           RowBox[{"N", "[", 
            RowBox[{"Cos", "[", 
             RowBox[{"j", " ", 
              RowBox[{"\[Pi]", "/", "n"}]}], "]"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "0", ",", "n"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xx", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"x", "[", "j", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "1", ",", 
             RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "construction", " ", "of", " ", "the", " ", "Chebyshev", " ", 
          "differentiation", " ", "matrix"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"0", ",", "0"}], "]"}], "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", 
             SuperscriptBox["n", "2"]}], "+", "1"}], ")"}], "/", "6"}]}], ";",
         "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"0", ",", "n"}], "]"}], "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "/", "2"}], ")"}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", "1"}], ")"}], "n"]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"n", ",", "0"}], "]"}], "=", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"(", 
            RowBox[{"1", "/", "2"}], ")"}]}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", "1"}], ")"}], "n"]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"n", ",", "n"}], "]"}], "=", 
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"2", 
               SuperscriptBox["n", "2"]}], "+", "1"}], ")"}], "/", "6"}], 
           ")"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
