Cell[CellGroupData[{

Cell["\<\
Sphaleron configuration calculated using  the spectral method\
\>", "Subsection",
 CellChangeTimes->{{3.897903361977828*^9, 
  3.897903373727624*^9}},ExpressionUUID->"17e78920-2eb0-4b5d-809a-\
7ab428003993"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SphaleronHTM", "[", 
    RowBox[{
    "\[Rho]1_", ",", "\[Rho]2_", ",", "\[Rho]3_", ",", "\[Rho]4_", ",", 
     "\[Rho]5_"}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"energy", "=", "10"}], ";", 
        RowBox[{"count", "=", "0"}]}], ",", 
       RowBox[{
        RowBox[{"energy", ">", "3"}], "&&", 
        RowBox[{"count", "<", "10"}]}], ",", 
       RowBox[{"count", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\[Beta]", "=", 
         RowBox[{"1", "+", 
          RowBox[{"2", "\[Rho]3"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"n", "=", "60"}], ";", 
        RowBox[{"(*", 
         RowBox[{"number", " ", "of", " ", "grid", " ", "points"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"a", "=", "30"}], ";", 
        RowBox[{"(*", 
         RowBox[{"cut", "-", 
          RowBox[{
          "off", " ", "of", " ", "the", " ", "sphaleron", " ", "distance", 
           " ", "parameter", " ", "\[Xi]"}]}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"n", " ", "and", " ", "a", " ", "can", " ", "be", " ", 
           RowBox[{"adjusted", ".", " ", "Basically"}]}], ",", " ", 
          RowBox[{
          "to", " ", "evaluate", " ", "the", " ", "sphaleron", " ", 
           "configuration", " ", "with", " ", "large", " ", "\[Rho]1", " ", 
           "more", " ", "accurately"}], ",", " ", 
          RowBox[{
          "one", " ", "should", " ", "increase", " ", "n", " ", "and", " ", 
           "decrease", " ", "a"}]}], "*)"}], "\[IndentingNewLine]", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Chebyshev", " ", "grid", " ", "points"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"x", "[", "j", "]"}], "=", 
           RowBox[{"N", "[", 
            RowBox[{"Cos", "[", 
             RowBox[{"j", " ", 
              RowBox[{"\[Pi]", "/", "n"}]}], "]"}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "0", ",", "n"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xx", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"x", "[", "j", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "1", ",", 
             RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "construction", " ", "of", " ", "the", " ", "Chebyshev", " ", 
          "differentiation", " ", "matrix"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"0", ",", "0"}], "]"}], "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", 
             SuperscriptBox["n", "2"]}], "+", "1"}], ")"}], "/", "6"}]}], ";",
         "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"0", ",", "n"}], "]"}], "=", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "/", "2"}], ")"}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", "1"}], ")"}], "n"]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"n", ",", "0"}], "]"}], "=", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"(", 
            RowBox[{"1", "/", "2"}], ")"}]}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", "1"}], ")"}], "n"]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"u", "[", 
          RowBox[{"n", ",", "n"}], "]"}], "=", 
         RowBox[{"-", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"2", 
               SuperscriptBox["n", "2"]}], "+", "1"}], ")"}], "/", "6"}], 
           ")"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"0", ",", "j"}], "]"}], "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"2", 
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}], "j"]}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"1", "-", 
              RowBox[{"x", "[", "j", "]"}]}], ")"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"n", ",", "j"}], "]"}], "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", "2"}], 
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}], 
               RowBox[{"n", "+", "j"}]]}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"1", "+", 
              RowBox[{"x", "[", "j", "]"}]}], ")"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"i", ",", "0"}], "]"}], "=", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"(", 
              RowBox[{"1", "/", "2"}], ")"}]}], 
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}], "i"], "/", 
              RowBox[{"(", 
               RowBox[{"1", "-", 
                RowBox[{"x", "[", "i", "]"}]}], ")"}]}], ")"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"i", ",", "n"}], "]"}], "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "/", "2"}], ")"}], 
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}], 
               RowBox[{"n", "+", "i"}]], "/", 
              RowBox[{"(", 
               RowBox[{"1", "+", 
                RowBox[{"x", "[", "i", "]"}]}], ")"}]}], ")"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"u", "[", 
            RowBox[{"j", ",", "j"}], "]"}], "=", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"-", 
              RowBox[{"x", "[", "j", "]"}]}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"2", 
              RowBox[{"(", 
               RowBox[{"1", "-", 
                SuperscriptBox[
                 RowBox[{"x", "[", "j", "]"}], "2"]}], ")"}]}], ")"}]}]}], 
          ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"i", "\[NotEqual]", "j"}], ",", 
            RowBox[{
             RowBox[{"u", "[", 
              RowBox[{"i", ",", "j"}], "]"}], "=", 
             RowBox[{
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}], 
               RowBox[{"i", "+", "j"}]], "/", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"x", "[", "i", "]"}], "-", 
                RowBox[{"x", "[", "j", "]"}]}], ")"}]}]}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "1", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Diff", "=", 
         RowBox[{"N", "[", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"u", "[", 
             RowBox[{"i", ",", "j"}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "0", ",", "n"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "0", ",", "n"}], "}"}]}], "]"}], "]"}]}], ";", 
        " ", "\[IndentingNewLine]", 
        RowBox[{"Diffcore", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"Diff", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "j"}], "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "2", ",", "n"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "2", ",", "n"}], "}"}]}], "]"}]}], ";", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"Diffsq", "=", 
         RowBox[{"N", "[", 
          RowBox[{"Diff", ".", "Diff"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Diffsqcore", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"Diffsq", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "j"}], "]"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "2", ",", "n"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "2", ",", "n"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"ff", "=", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"f", "[", "j", "]"}], ",", 
